            <li>WorldFeatureNamerCommon</li>
            <li>NamerWorldObject_LAO_FloatingIslandBase</li>
        </include>
        <rulePack>
            <rulesStrings>
                <li>terrain_word(p=4)->[flying_terrain]</li>
