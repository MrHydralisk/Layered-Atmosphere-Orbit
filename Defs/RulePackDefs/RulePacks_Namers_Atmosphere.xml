<Defs>
    <RulePackDef>
        <defName>NamerWorldObject_LAO_FloatingIslandBase</defName>
        <rulePack>
            <rulesStrings>
                <li>flying_terrain->[flying] [terrain]</li>
                <li>flying->floating</li>
                <li>flying->aerial</li>
                <li>flying->hovering</li>
                <li>flying->flying</li>
                <li>flying->sky</li>
                <li>flying->skyborne</li>
                <li>flying->airborne</li>
                <li>terrain->island</li>
                <li>terrain->land</li>
                <li>terrain->landform</li>
                <li>terrain->landmass</li>
                <li>terrain->isle</li>
				<li>size->Giant</li>
				<li>size->Massive</li>
				<li>size->Enormous</li>
				<li>size->Big</li>
                <li>typeMountain->Mountain</li>
                <li>typeMountain->Peak</li>
                <li>typeMountain->Alpine</li>
                <li>typeHill->Hill</li>
                <li>typeHill->Knoll</li>
                <li>typeHill->Mound</li>
                <li>typeFlat->Plain</li>
                <li>typeFlat->Lowland</li>
                <li>typeFlat->Flatland</li>
                <li>typeCrust->Crust</li>
                <li>typeCrust->Caves</li>
                <li>typeCrust->Cavern</li>
                <li>typeInfested->Infested</li>
                <li>typeInfested->Swarming</li>
                <li>typeInfested->Teeming</li>
                <li>typeCollapsing->Collapsing</li>
                <li>typeCollapsing->Crumbling</li>
                <li>typeCollapsing->Disintegrating</li>
                <li>typeCollapsing->Falling apart</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>
	
	
	
    <RulePackDef>
        <defName>NamerWorldObject_LAO_FloatingIsland</defName>
