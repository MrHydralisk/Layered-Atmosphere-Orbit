<?xml version="1.0" encoding="utf-8"?>
<Defs>
  
	<WorldObjectDef ParentName="GeneratedAsteroid" Name="Generated_LAO_FloatingIsland" Abstract="True">
		<worldObjectClass>LayeredAtmosphereOrbit.FloatingIslandMapParent</worldObjectClass>
		<nameMaker>NamerWorldObject_AsteroidBasic</nameMaker>
		<fullyExpandedInSpace>false</fullyExpandedInSpace>
		<modExtensions>
			<li Class="LayeredAtmosphereOrbit.LayeredAtmosphereOrbitDefModExtension">
				<availableBiomes>
					<li>BorealForest</li>
					<li>Tundra</li>
					<li>ColdBog</li>
					<li>IceSheet</li>
					<li>SeaIce</li>
					<li>TemperateForest</li>
					<li>TemperateSwamp</li>
					<li>TropicalRainforest</li>
					<li>TropicalSwamp</li>
					<li>AridShrubland</li>
					<li>Desert</li>
					<li>ExtremeDesert</li>
					<li MayRequire="Ludeon.RimWorld.Odyssey">GlacialPlain</li>
					<li MayRequire="Ludeon.RimWorld.Odyssey">Grasslands</li>
					<li MayRequire="Ludeon.RimWorld.Odyssey">Scarlands</li>
				</availableBiomes>
			</li>
		</modExtensions>
	</WorldObjectDef>

	<!-- Atmosphere -->
	
	<WorldObjectDef ParentName="SpaceBase">
		<defName>LAO_Atmosphere</defName>
		<label>atmosphere</label>
		<worldObjectClass>LayeredAtmosphereOrbit.AtmosphereMapParent</worldObjectClass>
		<canResizeToGravship>true</canResizeToGravship>
	</WorldObjectDef>

	<WorldObjectDef ParentName="Generated_LAO_FloatingIsland">
		<defName>LAO_FloatingIslandGiantMountain</defName>
		<label>giant floating island (mountain)</label>
		<description>A giant landmass that was lifted off the planet's surface. Previously, it was the peak of a mountain in the region below it.</description>
		<expandingIconTexture>World/WorldObjects/Expanding/LAO_FloatingIslandMountain</expandingIconTexture>
		<mapGenerator>LAO_FloatingIslandGiantMountain</mapGenerator>
		<overrideMapSize>(350,1,350)</overrideMapSize>
		<expandingIconDrawSize>1.6</expandingIconDrawSize>
	</WorldObjectDef>

	<WorldObjectDef ParentName="Generated_LAO_FloatingIsland">
		<defName>LAO_FloatingIslandGiantInfested</defName>
		<label>giant floating island (crust)</label>
		<description>A giant landmass that was lifted off the planet's surface. Previously, it was a heavily infested section deep in the planet's crust.</description>
		<expandingIconTexture>World/WorldObjects/Expanding/LAO_FloatingIslandInfested</expandingIconTexture>
		<mapGenerator>LAO_FloatingIslandGiantInfested</mapGenerator>
		<overrideMapSize>(350,1,350)</overrideMapSize>
		<expandingIconDrawSize>1.6</expandingIconDrawSize>
	</WorldObjectDef>

	<WorldObjectDef ParentName="Generated_LAO_FloatingIsland">
		<defName>LAO_FloatingIslandGiantFlat</defName>
		<label>giant floating island (plain)</label>
		<description>A giant landmass that was lifted off the planet's surface. Previously, it was a section of an open plain on the planet's surface below it.</description>
		<expandingIconTexture>World/WorldObjects/Expanding/LAO_FloatingIslandFlat</expandingIconTexture>
		<mapGenerator>LAO_FloatingIslandGiantFlat</mapGenerator>
		<overrideMapSize>(350,1,350)</overrideMapSize>
		<expandingIconDrawSize>1.6</expandingIconDrawSize>
	</WorldObjectDef>
	
	

	<WorldObjectDef ParentName="Generated_LAO_FloatingIsland">
		<defName>LAO_FloatingIslandHill</defName>
		<label>floating island (hill)</label>
		<description>A landmass that was lifted off the planet's surface. Previously, it was the peak of a hill in the region below it.</description>
		<expandingIconTexture>World/WorldObjects/Expanding/LAO_FloatingIslandMountain</expandingIconTexture>
		<mapGenerator>LAO_FloatingIslandHill</mapGenerator>
		<expandingIconDrawSize></expandingIconDrawSize>
	</WorldObjectDef>

	<WorldObjectDef ParentName="Generated_LAO_FloatingIsland">
		<defName>LAO_FloatingIslandFlat</defName>
		<label>floating island (plain)</label>
		<description>A landmass that was lifted off the planet's surface. Previously, it was a section of an open plain on the planet's surface below it.</description>
		<expandingIconTexture>World/WorldObjects/Expanding/LAO_FloatingIslandFlat</expandingIconTexture>
		<mapGenerator>LAO_FloatingIslandFlat</mapGenerator>
	</WorldObjectDef>
	
	

	<WorldObjectDef ParentName="Generated_LAO_FloatingIsland">
		<defName>LAO_FloatingIslandCollapsing</defName>
		<label>collapsing floating island</label>
		<description>A landmass that was lifted off the planet's surface. Over time, it began to erode and fall apart, eventually revealing a hidden structure within.</description>
		<expandingIconTexture>World/WorldObjects/Expanding/LAO_FloatingIslandCollapsing</expandingIconTexture>
		<mapGenerator>LAO_FloatingIslandCollapsing</mapGenerator>
	</WorldObjectDef>

	<WorldObjectDef ParentName="Generated_LAO_FloatingIsland">
		<defName>LAO_FloatingIslandDebug</defName>
		<label>floating island debug</label>
		<expandingIconTexture>World/WorldObjects/Expanding/LAO_FloatingIslandMountain</expandingIconTexture>
		<mapGenerator>LAO_FloatingIslandDebug</mapGenerator>
	</WorldObjectDef>
	
	<!-- Orbit -->

	<WorldObjectDef ParentName="GeneratedAsteroid">
		<defName>LAO_AsteroidPrecious</defName>
		<label>precious asteroid</label>
		<description>An asteroid orbiting the planet, containing precious materials.</description>
		<nameMaker>NamerWorldObject_AsteroidBasic</nameMaker>
		<mapGenerator>LAO_AsteroidPrecious</mapGenerator>
		<overrideMapSize>(350,1,350)</overrideMapSize>
		<expandingIconDrawSize>1.6</expandingIconDrawSize>
	</WorldObjectDef>

</Defs>
